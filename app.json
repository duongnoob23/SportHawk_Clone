{
  "expo": {
    "name": "SportHawk",
    "slug": "sporthawkmvpv4",
    "scheme": "sporthawk",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
