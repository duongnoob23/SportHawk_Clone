      "bundleIdentifier": "com.sporthawk.sporthawkmvpv4",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      },
      "entitlements": {
        "aps-environment": "production"
      },
      "googleServicesFile": "./constants/keys/GoogleService-Info.plist"
    },
    "android": {
      "package": "com.sporthawk.mvp_v4",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-square3.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./constants/keys/google-services.json",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      [
        "react-native-maps",
        {
          "iosGoogleMapsApiKey": "AIzaSyAy4-6kdoixUBA__Qa2yjH4iX6k21btm74",
          "androidGoogleMapsApiKey": "AIzaSyAy4-6kdoixUBA__Qa2yjH4iX6k21btm74"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "forceStaticLinking": ["RNFBApp"]
