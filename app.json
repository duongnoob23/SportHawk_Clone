        }
      ],
      "expo-secure-store",
      "expo-notifications",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c6c282ae-91d0-4457-a081-4728123642f8"
      }
    },
    "owner": "sporthawk"
  }
